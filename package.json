  "scripts": {
    "analyze": "dotenvx run -- nuxt analyze",
    "build": "dotenvx run -- nuxt build",

    "cleanup": "nuxt cleanup",

    "dev": "nuxt dev --no-clear --no-fork",

    "lint": "run-p lint:oxlint lint:eslint",
    "lint:fix": "run-s lint:fix:oxlint lint:fix:eslint",

    "lint:eslint": "eslint --cache --cache-location .eslintcache --concurrency=auto .",
    "lint:fix:eslint": "eslint --concurrency=auto --fix .",

    "lint:oxlint": "oxlint --type-aware .",
    "lint:fix:oxlint": "oxlint --fix --type-aware .",

    "format": "eslint -c eslint.stylistic.config.ts --concurrency=auto --fix .",

    "preview": "dotenvx run -- nuxt preview --port 88",
    "start": "dotenvx run -- node .output/server/index.mjs",

    "seed": "dotenvx run -- node --experimental-strip-types init/seed_db.ts",

    "typecheck": "nuxt typecheck"
  },
  "type": "module",
  "version": "1.5.0"
